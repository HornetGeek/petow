[ignore]
; We fork some components by platform
.*/*[.]android.js

; Ignore "BUCK" generated dirs
<PROJECT_ROOT>/\.buckd/

; Ignore unexpected extra files
<PROJECT_ROOT>/\.buckconfig.local
<PROJECT_ROOT>/\.buckversion
<PROJECT_ROOT>/\.flowconfig.local

; Ignore the build output
<PROJECT_ROOT>/android/app/build
<PROJECT_ROOT>/ios/build

; Ignore CocoaPods
<PROJECT_ROOT>/ios/Pods

[include]

[libs]
<PROJECT_ROOT>/node_modules/react-native/Libraries/react-native/react-native-interface.js
<PROJECT_ROOT>/node_modules/react-native/type-definitions/react-native.d.ts

[options]
emoji=true

module.system=haste

experimental.strict_type_args=true

munge_underscores=true

module.name_mapper='^[./a-zA-Z0-9$_-]+\.\(bmp\|gif\|jpg\|jpeg\|png\|psd\|svg\|webp\|m4v\|mov\|mp4\|mpeg\|mpg\|webm\|aac\|aiff\|caf\|m4a\|mp3\|wav\|html\|pdf\)$' -> 'RelativeImageStub'

suppress_type=$FlowFixMe
suppress_type=$FlowExpectedError

[untyped]
.*/node_modules/@react-native-community/cli-platform-ios/native_modules\.js

[version]
^0.0.0
